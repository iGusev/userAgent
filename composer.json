{
    "name": "useragent/useragent",
    "description": ":package_description",
    "keywords": [
        "league",
        "userAgent"
    ],
    "homepage": "https://github.com/thephpleague/userAgent",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Gusev",
            "email": "mail@igusev.ru",
            "homepage": "https://php-cat.com",
            "role": "Developer"
        }
